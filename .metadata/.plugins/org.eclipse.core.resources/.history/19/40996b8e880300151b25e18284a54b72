package com.example.parse;

import android.os.Bundle;
import android.support.v7.app.ActionBarActivity;
import android.view.View;
import android.widget.Toast;

import com.parse.Parse;
import com.parse.ParseObject;

public class MainActivity extends ActionBarActivity {

	@Override
	protected void onCreate(Bundle savedInstanceState) {
		super.onCreate(savedInstanceState);
		setContentView(R.layout.activity_main);
	}


	public void click(View v)
	{
		Parse.initialize(this, "YRg6LkthSFL3sXBdc99VqQmUwoOi4fgBKoYnalXC", "3ZPB7eUviJ8DEyVbasJgWakLGMd6UqLGcYIjb0k8");
		ParseObject obj=new ParseObject("POST");
		obj.add("name", "appu");
		obj.add("place", "Thekkemala");
		obj.add("address", "Kaithavana");
		obj.saveEventually();
		Toast.makeText(getApplicationContext(), "Hey there!", Toast.LENGTH_SHORT).show();
	}


}
